<table id="example" class="table table-striped table-bordered table-responsive" cellspacing="0" width="100%">
  <thead> 
    <tr>
      {{#each data.headers as |header|}}
          <th>{{header}}</th> 
      {{/each}}
    </tr> 
  </thead> 
  <tbody> 
  {{#each data.result as |issue|}}
    <tr>
      {{#each-in issue as |category values|}}
          <td>{{values}}</td>
      {{/each-in}}
    </tr> 
  {{/each}}
  </tbody>
</table>
